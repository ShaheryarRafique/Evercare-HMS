<div class="row">

    <div class="col-md-6">
        <div class="form-group">
            <label>Admin ID <span class="text-danger">*</span></label>
            <input class="form-control" type="text" value="{{this.admin_id}}" name="admin_id" disabled>
        </div>
    </div>

    <div class="col-sm-6">
        <div class="form-group">
            <label class="form-label require">Admin Name <span class="text-danger">*</span></label>
            <input class="form-control" type="text" name="name" value="{{this.name}}" required>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <label>Email <span class="text-danger">*</span></label>
            <input class="form-control" type="text" value="{{this.email}}" name="email" required>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <label>Password <span class="text-danger">*</span></label>
            <input class="form-control" type="text" value="{{this.password}}" name="password" required>
        </div>
    </div>


</div>